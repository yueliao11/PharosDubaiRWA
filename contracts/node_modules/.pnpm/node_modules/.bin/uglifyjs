#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/uglify-js@3.19.3/node_modules/uglify-js/bin/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/uglify-js@3.19.3/node_modules/uglify-js/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/uglify-js@3.19.3/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/uglify-js@3.19.3/node_modules/uglify-js/bin/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/uglify-js@3.19.3/node_modules/uglify-js/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/uglify-js@3.19.3/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
fi
