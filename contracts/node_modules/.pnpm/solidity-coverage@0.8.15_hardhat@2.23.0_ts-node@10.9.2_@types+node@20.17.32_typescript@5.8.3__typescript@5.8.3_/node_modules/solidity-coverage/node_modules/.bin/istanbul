#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/lib/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/lib/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules/sc-istanbul/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/sc-istanbul@0.4.6/node_modules:/Volumes/extdisk/project/web3/PharosDubaiRWA/contracts/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../sc-istanbul@0.4.6/node_modules/sc-istanbul/lib/cli.js" "$@"
else
  exec node  "$basedir/../../../../../sc-istanbul@0.4.6/node_modules/sc-istanbul/lib/cli.js" "$@"
fi
